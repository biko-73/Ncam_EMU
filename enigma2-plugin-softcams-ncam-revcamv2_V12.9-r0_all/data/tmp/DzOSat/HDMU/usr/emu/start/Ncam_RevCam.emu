name = RevcamV2_Ncam
binname = ncam
binname = revcamv2
start = /usr/emu/ncam -d -c /etc/tuxbox/config/ncam.conf | /usr/bin/revcamv2 /etc/tuxbox/config/RevcamV2/oscam.conf &
stop = killall -9 ncam
stop = killall -9 revcamv2

